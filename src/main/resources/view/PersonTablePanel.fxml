<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>

<VBox xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
  <TableView fx:id="personTableView" VBox.vgrow="ALWAYS">
    <columns>
      <TableColumn fx:id="indexColumn" text="Index" prefWidth="50" />
      <TableColumn fx:id="nameColumn" text="Name" prefWidth="150" />
      <TableColumn fx:id="studentIdColumn" text="StudentID" prefWidth="110" />
      <TableColumn fx:id="emailColumn" text="Email" prefWidth="200" />
      <TableColumn fx:id="phoneColumn" text="Phone" prefWidth="100" />
      <TableColumn fx:id="tagsColumn" text="Tags" prefWidth="150" />
      <TableColumn fx:id="attendanceStatusColumn" text="Attendance" />
    </columns>
  </TableView>
</VBox>
